{
  "builds": [
    {
      "src": "frontend-react/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "installCommand": "npm ci && npm --prefix frontend-react ci",
  "buildCommand": "npm --prefix frontend-react run build",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
